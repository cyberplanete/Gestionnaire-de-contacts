<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<DialogPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="contactApp.ProcessNouveauContact">
    <headerText>Ajout d'un contact</headerText>
    <content>
        <GridPane vgap="10" hgap="10" >
            <Label text="Nom :" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="champNom" prefHeight="26.0" prefWidth="313.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Label text="Prénom :" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="champPrénom" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Téléphone :" GridPane.columnIndex="0" GridPane.rowIndex="2" />
         	<TextField fx:id="champTéléphone" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         <Label text="Note :" GridPane.rowIndex="3" />
         <TextField fx:id="champNotes" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         
         </GridPane>
         
    </content>
</DialogPane>
